imulation=simulation)\\n        \\n        # Create liquid handler instance\\n        self.lh = LiquidHandler(backend=backend)\\n        \\n        # Setup the deck\\n        deck = STARDeck()\\n        await self.lh.setup(deck=deck)\\n        \\n        print(f\"✓ Liquid handler setup complete (Simulation: {simulation})\")\\n        \\n    def setup_labware(self):\\n        \"\"\"Setup the labware on the deck.\"\"\"\\n        print(\"Setting up labware...\")\\n        \\n        # Create source plate (96-well deep well plate)\\n        self.source_plate = Cos_96_DW_2mL_P(name=\"source_plate\")\\n        \\n        # Create destination plate (96-well deep well plate)  \\n        self.dest_plate = Cos_96_DW_2mL_P(name=\"dest_plate\")\\n        \\n        # Create tip rack (1000µL tips)\\n        self.tip_rack = TIP_CAR_480_HTF_A00(name=\"tip_rack\")\\n        \\n        # Assign positions on deck\\n        # Position the labware at specific deck locations\\n        self.lh.deck.assign_child_resource(self.source_plate, location=Coordinate(100, 100, 0))\\n        self.lh.deck.assign_child_resource(self.dest_plate, location=Coordinate(200, 100, 0))  \\n        self.lh.deck.assign_child_resource(self.tip_rack, location=Coordinate(300, 100, 0))\\n        \\n        print(\"✓ Labware setup complete\")\\n        print(f\"  - Source plate: {self.source_plate.name} at position (100, 100, 0)\")\\n        print(f\"  - Destination plate: {self.dest_plate.name} at position (200, 100, 0)\")\\n        print(f\"  - Tip rack: {self.tip_rack.name} at position (300, 100, 0)\")\\n        \\n    def define_well_ranges(self, source_range: str, dest_range: str) -> Tuple[List, List]:\\n        \"\"\"\\n        Define source and destination well ranges.\\n        \\n        Args:\\n            source_range (str): Source well range (e.g., \"A1:H1\", \"A1:A12\")\\n            dest_range (str): Destination well range (e.g., \"A1:H1\", \"A1:A12\")\\n            \\n        Returns:\\n            Tuple[List, List]: Lists of source and destination wells\\n        \"\"\"\\n        print(f\"Defining well ranges...\")\\n        print(f\"  Source range: {source_range}\")\\n        print(f\"  Destination range: {dest_range}\")\\n        \\n        # Parse well ranges and get well objects\\n        source_wells = self.source_plate.get_wells(source_range)\\n        dest_wells = self.dest_plate.get_wells(dest_range)\\n        \\n        if len(source_wells) != len(dest_wells):\\n            raise ValueError(f\"Source wells ({len(source_wells)}) and destination wells ({len(dest_wells)}) count mismatch\")\\n            \\n        print(f\"✓ Well ranges defined: {len(source_wells)} transfers planned\")\\n        \\n        return source_wells, dest_wells\\n        \\n    async def transfer_water(self, \\n                           source_wells: List, \\n                           dest_wells: List, \\n                           volume: float = 100.0,\\n                           use_new_tips: bool = True):\\n        \"\"\"\\n        Transfer water from source wells to destination wells.\\n        \\n        Args:\\n            source_wells (List): List of source well objects\\n            dest_wells (List): List of destination well objects  \\n            volume (float): Volume to transfer in µL\\n            use_new_tips (bool): Whether to use new tips for each transfer\\n        \"\"\"\\n        print(f\"Starting water transfer protocol...\")\\n        print(f\"  Volume per transfer: {volume} µL\")\\n        print(f\"  Number of transfers: {len(source_wells)}\")\\n        print(f\"  Use new tips: {use_new_tips}\")\\n        \\n        for i, (source_well, dest_well) in enumerate(zip(source_wells, dest_wells), 1):\\n            print(f\"\\\\\\\\nTransfer {i}/{len(source_wells)}: {source_well.name} → {dest_well.name}\")\\n            \\n            if use_new_tips:\\n                # Pick up fresh tips\\n                await self.lh.pick_up_tips(self.tip_rack[0:1])  # Pick up 1 tip\\n                \\n            # Aspirate from source well\\n            print(f\"  Aspirating {volume} µL from {source_well.name}\")\\n            await self.lh.aspirate([source_well], volume_uL=[volume])\\n            \\n            # Dispense to destination well\\n            print(f\"  Dispensing {volume} µL to {dest_well.name}\")\\n            await self.lh.dispense([dest_well], volume_uL=[volume])\\n            \\n            if use_new_tips:\\n                # Drop tips\\n                await self.lh.drop_tips()\\n                \\n        print(f\"\\\\\\\\n✓ Water transfer protocol completed successfully!\")\\n        \\n    async def run_protocol(self, \\n                         source_range: str = \"A1:A6\", \\n                         dest_range: str = \"B1:B6\",\\n                         volume: float = 100.0,\\n                         port: str = \"COM1\",\\n                         simulation: bool = True):\\n        \"\"\"\\n        Run the complete water transfer protocol.\\n        \\n        Args:\\n            source_range (str): Source well range\\n            dest_range (str): Destination well range  \\n            volume (float): Volume to transfer per well in µL\\n            port (str): Hamilton STAR communication port\\n            simulation (bool): Run in simulation mode\\n        \"\"\"\\n        try:\\n            print(\"=\"*60)\\n            print(\"HAMILTON STAR WATER TRANSFER PROTOCOL\")\\n            print(\"=\"*60)\\n            \\n            # Setup liquid handler\\n            await self.setup_liquid_handler(port=port, simulation=simulation)\\n            \\n            # Setup labware\\n            self.setup_labware()\\n            \\n            # Define well ranges\\n            source_wells, dest_wells = self.define_well_ranges(source_range, dest_range)\\n            \\n            # Execute transfer\\n            await self.transfer_water(source_wells, dest_wells, volume=volume)\\n            \\n        except Exception as e:\\n            print(f\"❌ Protocol failed with error: {e}\")\\n            raise\\n        finally:\\n            # Cleanup\\n            if self.lh:\\n                print(\"\\\\\\\\nCleaning up...\")\\n                await self.lh.stop()\\n                print(\"✓ Liquid handler stopped\")\\n\\n# Example usage functions\\nasync def example_basic_transfer():\\n    \"\"\"Example: Basic water transfer from column A to column B\"\"\"\\n    protocol = WaterTransferProtocol()\\n    await protocol.run_protocol(\\n        source_range=\"A1:A6\",  # Wells A1 through A6\\n        dest_range=\"B1:B6\",    # Wells B1 through B6  \\n        volume=150.0,          # 150 µL per transfer\\n        simulation=True        # Run in simulation mode\\n    )\\n\\nasync def example_row_transfer():\\n    \"\"\"Example: Transfer entire row\"\"\"\\n    protocol = WaterTransferProtocol()\\n    await protocol.run_protocol(\\n        source_range=\"A1:H1\",  # Entire row A\\n        dest_range=\"A2:H2\",    # Entire row B\\n        volume=200.0,          # 200 µL per transfer\\n        simulation=True\\n    )\\n\\nasync def example_custom_transfer():\\n    \"\"\"Example: Custom well selection\"\"\"\\n    protocol = WaterTransferProtocol()\\n    \\n    # Setup protocol\\n    await protocol.setup_liquid_handler(simulation=True)\\n    protocol.setup_labware()\\n    \\n    # Define custom wells (you can specify individual wells)\\n    source_wells = [\\n        protocol.source_plate[\"A1\"],\\n        protocol.source_plate[\"C3\"], \\n        protocol.source_plate[\"E5\"]\\n    ]\\n    dest_wells = [\\n        protocol.dest_plate[\"B2\"],\\n        protocol.dest_plate[\"D4\"],\\n        protocol.dest_plate[\"F6\"] \\n    ]\\n    \\n    # Execute custom transfer\\n    await protocol.transfer_water(source_wells, dest_wells, volume=75.0)\\n    \\n    # Cleanup\\n    await protocol.lh.stop()\\n\\n# Main execution\\nasync def main():\\n    \"\"\"Main function to run the protocol.\"\"\"\\n    print(\"PyLabRobot Hamilton STAR Water Transfer Script\")\\n    print(\"Choose an example to run:\")\\n    print(\"1. Basic column transfer (A1:A6 → B1:B6)\")\\n    print(\"2. Row transfer (A1:H1 → A2:H2)\")  \\n    print(\"3. Custom well transfer\")\\n    \\n    # For demonstration, run the basic transfer\\n    print(\"\\\\\\\\nRunning basic column transfer example...\")\\n    await example_basic_transfer()\\n\\nif __name__ == \"__main__\":\\n    # Run the protocol\\n    asyncio.run(main())\\n\\'\\'\\'\\n\\n# Save the script to a file\\nwith open(\"hamilton_star_water_transfer.py\", \"w\") as f:\\n    f.write(script_content)\\n\\nprint(\"✓ Complete Hamilton STAR water transfer script created!\")\\nprint(\"✓ Saved as: hamilton_star_water_transfer.py\")\\nprint(\"\\\\nScript features:\")\\nprint(\"- Hamilton STAR backend integration\")\\nprint(\"- Configurable source and destination well ranges\") \\nprint(\"- Volume control\")\\nprint(\"- Simulation mode for testing\")\\nprint(\"- Error handling and cleanup\")\\nprint(\"- Multiple example protocols\")\\n</execute>',\n",
